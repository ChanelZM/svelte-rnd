{
	"compilerOptions": {
		"paths": {
			"$services/*": [
				"../src/core/services/*"
			],
			"$components/*": [
				"../src/components/*"
			],
			"$connectors/*": [
				"../src/connectors/*"
			],
			"$stores/*": [
				"../src/core/stores/*"
			],
			"$utils/*": [
				"../src/utils/*"
			],
			"$models/*": [
				"../src/core/models/*"
			],
			"$styles/*": [
				"../src/styles/*"
			],
			"$examples/*": [
				"../examples/*"
			],
			"$mixins/*": [
				"../src/styles/mixins/*"
			],
			"$static/*": [
				"../static/*"
			],
			"$src/*": [
				"../src/*"
			]
		},
		"rootDirs": [
			"..",
			"./types"
		],
		"verbatimModuleSyntax": true,
		"isolatedModules": true,
		"lib": [
			"esnext",
			"DOM",
			"DOM.Iterable"
		],
		"moduleResolution": "bundler",
		"module": "esnext",
		"noEmit": true,
		"target": "esnext"
	},
	"include": [
		"ambient.d.ts",
		"non-ambient.d.ts",
		"./types/**/$types.d.ts",
		"../vite.config.js",
		"../vite.config.ts",
		"../src/**/*.js",
		"../src/**/*.ts",
		"../src/**/*.svelte",
		"../tests/**/*.js",
		"../tests/**/*.ts",
		"../tests/**/*.svelte"
	],
	"exclude": [
		"../node_modules/**",
		"../src/service-worker.js",
		"../src/service-worker/**/*.js",
		"../src/service-worker.ts",
		"../src/service-worker/**/*.ts",
		"../src/service-worker.d.ts",
		"../src/service-worker/**/*.d.ts"
	]
}